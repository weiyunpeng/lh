{
    "name": "lianghui",
    "version": "1.0.0",
    "description": "",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dev": "electron . --debug",
        "package": "electron-packager .  lianghui --overwrite --all --out=out --icon=favicon.ico --version 1.0.0 --overwrite --no-package-manager --ignore='(.git|node_modules)'",
        "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=favicon.ico --osx-sign.identity='Developer ID Application: GitHub' --overwrite --no-package-manager --ignore='(.git|node_modules)'",
        "package:win32": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=favicon.ico --overwrite --no-package-manager --ignore='(.git|node_modules)'",
        "package:win64": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=out --icon=favicon.ico --overwrite --no-package-manager --ignore='(.git|node_modules)'",
        "package:linux": "electron-packager . lianghui --overwrite --platform=linux --arch=x64 --out=out --icon=favicon.ico --overwrite --no-package-manager --ignore='(.git|node_modules)'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/weiyunpeng/lh.git"
    },
    "bugs": {
        "url": "https://github.com/weiyunpeng/lh/issues"
    },
    "author": "Mr.Ypeng",
    "license": "MIT",
    "devDependencies": {
        "devtron": "^1.3.0",
        "electron": "~1.7.8",
        "electron-packager": "^8.6.0",
        "electron-winstaller": "^2.2.0",
        "husky": "^0.14.3",
        "mocha": "^3.1.0",
        "npm-run-all": "^4.0.2",
        "request": "^2.70.0"
    },
    "dependencies": {
        "tracking": "^1.1.3"
    }
}
